{
  "name": "frontend",
  "version": "1.0.0",
  "main": "assets/scripts/main.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "description": "",
  "scripts": {
    "graph": "madge -i graph.svg assets/scripts/main.js",
    "style": "sass --watch --no-source-map assets/styles/main.scss:assets/styles/main.css",
    "local:front": "cross-env NODE_ENV=development MODE=local vite -c vite.config.js",
    "physic:front": "cross-env NODE_ENV=development MODE=physic vite -c vite.config.js",
    "prod:front": "cross-env NODE_ENV=development MODE=prod vite -c vite.config.js",
    "local": "concurrently --kill-others-on-fail \"bun run graph\" \"bun run style\" \"bun run local:front\"",
    "physic": "concurrently --kill-others-on-fail \"bun run graph\" \"bun run style\" \"bun run physic:front\"",
    "prod": "concurrently --kill-others-on-fail \"bun run graph\" \"bun run style\" \"bun run prod:front\"",
    "build": "vite build"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "protobufjs": "^7.1.2",
    "uuid": "^9.0.0",
    "vite": "^4.0.1"
  },
  "devDependencies": {}
}
